<h1> This is where all the artists who match the search by craft and location </h1>
  <div class="container" id="artists" >
    <div class="row">
       <% @users.each do |user| %>
          <%= "First Name:" link_to user.first_name, artist_path(user) %>
          <%= "Last Name:" user.last_name %>
          <%= "Nickname:" user.nickname %>
          <%= "Phone number:" user.phone_number %>
          <%= "Location:" user.location %>
          <%= "Language:" user.language %>
          <%= "Email:" user.email %>
      <% end %>
    </div>
  </div>
</div>
